include_directories(${QT_INCLUDE_DIR} ${CMAKE_CURRENT_BINARY_DIR})
set(
    RegisterApp_MOC_HDRS
    RegisterApp.h
)
qt4_wrap_cpp(
    RegisterApp_SRCS
    ${RegisterApp_MOC_HDRS}
)

add_library(
    RegisterApp
    MODULE
    RegisterApp.cpp
    ${RegisterApp_MOC_SRCS}
)

target_link_libraries(
    RegisterApp
    ${QT_QTCORE_LIBRARY}
)